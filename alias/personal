alias cgdb="cgdb --quiet"
alias db="mysql --silent --sigint-ignore --table"
alias dbs="mysql --silent --skip-column-names"
alias file="file -sL"
alias g++="g++ -std=c++11 -Wall -Wextra -Wpedantic -ggdb -pthread -rdynamic"
alias g=git
alias gcc="gcc            -Wall -Wextra -Wpedantic -ggdb -pthread -rdynamic"
alias gdb="gdb --quiet"
alias m="make"
alias OFF="sudo systemctl poweroff --force --force"
alias t="cd ~/torrent; clear; ls -gG"
alias vo="vi -O"

alias root="if screen -S root -Q select . >& /dev/null; then screen -S root -r; else screen -S root su -; fi"
alias work="cddir work/dparted; clear; git sb"
alias music="cd music; clear; ls"
alias backup="cd backup; clear; ls"
alias eg="cd examples; clear; ls -C"
alias tv="cd ~/tv; clear; ls"
alias ..="cd .."
alias .="cd ."
alias ~="cd $HOME; clear; ls"
