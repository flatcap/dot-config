alias !="history2"
alias bc="bc --quiet --mathlib"
alias bzip2="_compress /usr/bin/bzip2"
alias ct="column -t"
alias date2='date "+%n  %R  %A %-d %B %Y%n"'
alias df="df --print-type --exclude-type rootfs --exclude-type devtmpfs --exclude-type tmpfs"
alias diff="diff --exclude CVS --exclude .svn --exclude tags --exclude .git"
alias gthumb="_wrapdot '/usr/bin/gthumb'"
alias jobs="jobs -l"
alias keep="export IGNOREEOF=999"
alias killall="killall --ignore-case"
alias man="_wrapsimple /usr/bin/man"
alias mkdir="mkdir --parents"
alias nautilus="_wrapdot /usr/bin/nautilus"
alias od="LANG=C hexdump -f ~/.format"
alias odw="LANG=C hexdump -f ~/.format2"
alias rar="unrar"
alias rmdir="rmdir --ignore-fail-on-non-empty --parents"
alias ssh="ssh -q"
alias tar="_wrapsimple /bin/tar"
alias tree="tree -C"
alias vi="vim -X"
alias xz="_compress /usr/bin/xz"
