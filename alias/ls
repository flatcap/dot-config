alias l1="/bin/ls --group-directories-first -I .\*swp -I lost+found -1"
alias la="/bin/ls --group-directories-first -I .\*swp -I lost+found --color=auto -T 0 -la"
alias lc="/bin/ls --group-directories-first -I .\*swp -I lost+found --color=auto -T 0 -CL"
alias lh="/bin/ls --group-directories-first -I .\*swp -I lost+found --color=auto -T 0 -lh"
alias ll="/bin/ls --group-directories-first -I .\*swp -I lost+found --color=auto -T 0 -l"
alias ls="/bin/ls --group-directories-first -I .\*swp -I lost+found --color=auto -T 0 -lL"
alias lz="/bin/ls --group-directories-first -I .\*swp -I lost+found --color=auto -T 0 -lL --context"
