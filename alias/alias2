alias l1="/bin/ls --group-directories-first -1"
alias la="/bin/ls --group-directories-first -la -T 0 --color=auto"
alias ll="/bin/ls --group-directories-first -l -T 0 --color=auto"
alias ls="/bin/ls --group-directories-first -lL -T 0 --color=auto -I .\*swp -I lost+found"

alias !="history2"
alias bc="bc -q"
alias bzip2="_compress /usr/bin/bzip2"
alias cd="cd >& /dev/null"
alias date2='date "+%n  %R  %A %-d %B%n"'
alias db="mysql -st"
alias df="df_"
alias diff="diff -x CVS -x .svn -x tags -x .git"
alias easytag="_wrapdot /usr/bin/easytag"
alias ftp="lftp"
alias gthumb="_wrapdot /usr/bin/gthumb"
alias jobs="jobs -l"
alias keep="export IGNOREEOF=999"
alias killall="killall -I"

alias man="_wrapsimple /usr/bin/man"
alias more="less"
alias nautilus="_wrapdot /usr/bin/nautilus"
alias od="LANG=C hexdump -f ~/.format"
alias odw="LANG=C hexdump -f ~/.format2"
alias rar="unrar"
alias ssh="ssh -q"
alias ssh-helper='ssh-add $(find ~/.ssh -name \*.pub | sed "s/\.pub//")'
alias tar="_wrapsimple /bin/tar"
alias tree="tree -C"
alias vi="vim -X"

alias c="cd ~/downloads/craggy; clear; ls -gG"
alias d="cd ~/downloads; clear; ls -gG"
alias file='file --magic-file=/usr/share/file/magic:$HOME/.magic'
alias g="cd ~/downloads/guardian; clear;/list.sh"
alias yyum="yum -y"
